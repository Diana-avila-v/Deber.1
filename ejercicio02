Algoritmo expresion1
	a <- (5 + 3 * 2) + 9 > 3 * 5 * 14 % 3
	Escribir "La respuesta del ejercicio 1 es:",a
	
	b <- 2 *(4 - 10 + 8)/2* 36 *(1/2)
	Escribir "La respuesta del ejercicio 2 es:",b
	
	c <- 260 / 12 + 54 % 3 - 85 % 7
	Escribir "La respuesta del ejercicio 3 es:",c
	
	d <- (48 < 2 * 3) || (2 * 7 < 12)
	Escribir "La respuesta del ejercicio 4 es:",d
	
	e <- ((8 > 2) || (932 < 23) ) & 4 == 2
	Escribir "La respuesta del ejercicio 5 es:", e
FinAlgoritmo
